; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="ArenaFight"
config/icon="res://icon.png"

[autoload]

Data="*res://scripts/Data.gd"
Events="*res://scripts/Events.gd"
Global="*res://scripts/Global.gd"
Loader="*res://scripts/Loader.gd"

[display]

window/size/width=320
window/size/height=240
window/stretch/mode="2d"
window/stretch/aspect="keep"

[layer_names]

2d_render/layer_1="platform"
2d_physics/layer_1="platform"
2d_render/layer_2="player"
2d_physics/layer_2="player"
2d_render/layer_3="playerbullet"
2d_physics/layer_3="playerbullet"
2d_render/layer_4="enemy"
2d_physics/layer_4="enemy"
2d_render/layer_5="enemybullet"
2d_physics/layer_5="enemybullet"
2d_render/layer_6="collectable"
2d_physics/layer_6="collectable"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
